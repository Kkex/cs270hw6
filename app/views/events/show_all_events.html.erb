<table>
    <tr>
        <th>Name</th>
        <th>Description</th>
        <th>Start Date</th>
        <th>End Date</th>
    </tr>
        
        <% @events.each do |e| %>
        <tr>
            <td><% = e.name %> </td>
            <td><% = e.description %> </td>
            <td><% = "#{e.start_date}" %> </td>
            <td><% = "#{e.end_date}" %> </td>
        </tr>
        <% end %>
    
</table>